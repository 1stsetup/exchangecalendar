                                      label="&event.reminder.2hours.before.label;"
                                      length="2"
                                      origin="before"
                                      relation="START"
                                      unit="hours"/>
                            <menuitem id="reminder-12hours-menuitem"
                                      label="&event.reminder.12hours.before.label;"
                                      length="12"
                                      origin="before"
                                      relation="START"
                                      unit="hours"/>
                            <menuseparator id="reminder-hours-separator"/>
                            <menuitem id="reminder-1day-menuitem"
