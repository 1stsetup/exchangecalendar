<!DOCTYPE dialog [
    <!ENTITY % dtd1 SYSTEM "chrome://exchangecalendar/locale/sharedCalendarParser.dtd" > %dtd1;
]>

<?xml-stylesheet type="text/css" href="chrome://exchangecalendar/skin/sharedCalendarParser.css"?>

<overlay id="exchWebServiceMessengerOverlay"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">

  <script type="application/x-javascript"  src="chrome://exchangecalendar/content/sharedCalendarParser.js"/>

  <vbox id="messagepanebox">
    <vbox position="1">
      <vbox id="exchWebServiceSharedCalendarBox" collapsed="true" align="center" class="exchWebServiceSharedCalendarBox">
	<hbox>
		<image src="chrome://exchangecalendar/content/lightningexchangecalendar.png"/>
		<vbox>
			<label value="&exchWebService_vbox_label;"/>
			<label id="exchWebServiceSharedCalendarLabel" value="test"/>
		</vbox>
	        <button id="exchWebServiceSharedCalendarAddButton"  label="&exchWebService_button_label_add_calendar;" oncommand="exchWebService.hdrView.doAddSharedCalendar();"/>
	</hbox>

      </vbox>
    </vbox>
  </vbox>

</overlay>


